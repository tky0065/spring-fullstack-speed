{
  "name": "@enokdev/spring-fullstack-speed",
  "version": "1.0.0",
  "description": "Générateur rapide d'applications fullstack avec Spring Boot et divers frameworks frontend",
  "main": "index.js",
  "type": "module",
  "bin": {
    "sfs": "cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "prepublish": "npm run build",
    "start": "node cli.js",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "link": "npm link",
    "unlink": "npm unlink",
    "test:generators": "node --experimental-vm-modules node_modules/jest/bin/jest.js generators/",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js utils/",
    "test:templates": "npm run build && node dist/scripts/test-generation.js",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "yeoman-generator",
    "spring-boot",
    "fullstack",
    "react",
    "vue",
    "angular",
    "thymeleaf",
    "jte",
    "inertia"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/yosay": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.8.3",
    "yeoman-test": "^10.1.1"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "type-fest": "4.41.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "yaml": "2.8.0",
    "yeoman-environment": "^4.4.3",
    "yeoman-generator": "^7.5.1",
    "yosay": "^3.0.0"
  }
}
